{
    "name": "aws/aws-sdk-php-zf2",
    "description": "Zend Framework 2 Module that allows easy integration the AWS SDK for PHP",
    "type": "library",
    "license": "Apache-2.0",
    "keywords": [
        "aws",
        "zf2",
        "amazon",
        "zend",
        "s3",
        "dynamodb",
        "ec2",
        "cloudfront",
        "sqs"
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "aws/aws-sdk-php": "3.*",
        "laminas/laminas-filter": "^2.9.0",
        "laminas/laminas-servicemanager": "^2.7.0 || ^3.0",
        "laminas/laminas-session": "^2.7.0",
        "laminas/laminas-view": "^2.8",
        "laminas/laminas-dependency-plugin": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.*",
        "laminas/laminas-modulemanager": "2.7.*",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "AwsModule\\": "src/"
        },
        "classmap": [
            "./Module.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "AwsModule\\Tests\\": "tests/"
        }
    },
    "extra": {
        "zf": {
            "module": "AwsModule"
        }
    }
}
